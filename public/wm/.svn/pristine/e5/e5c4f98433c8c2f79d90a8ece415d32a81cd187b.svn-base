{include file="public/header"}
<link rel="stylesheet" type="text/css" href="__STATIC__/index/css/shimingrenzheng-unband.css">
    <style>
        input{
            background: white;
        }
        a:hover{
          text-decoration: none;
        }
    </style>
<body style="background:#EFEFEF">
<div class="contain"  style="height:520px;">
    <!--头部-->
    {include file="public/top_menu"}
    <!--头部-->
    <!--加空白-->
    <div style=" height: 70px;width: 100%">
    </div>
    <!--加空白-->
    <div class="row">
    </div>
    <form action="" class="form-horizontal">
        <!--实名认证-->
        <div class="form-group" style="margin-left: 0;margin-right: 0;">
            <label for="anzhi" class="col-xs-2 anzhi-label text-center" style="padding-left:0;padding-right:0;background: #fff;margin-bottom: 0">
                <span class="span-1 col-xs-9 text-left  col-xs-offset-1" style="padding-left: 0">实名认证</span>
            </label>
            <input type="text" placeholder=""  class="col-xs-10" id="anzhi">
        </div>
        <!--实名认证-->
        <!--账号-->
        <div class="form-group" style="margin-left: 0;margin-right: 0;">
            <label for="zhangH" class="col-xs-2 anzhi-label text-center" style="padding-left:0;padding-right:0;background: #fff;margin-bottom: 0">
                <span class="span-1 col-xs-9 text-left  col-xs-offset-1" style="padding-left: 0">账号</span>
            </label>
            <input type="text" value="bbc"  class="col-xs-10" id="zhangH" disabled   style="color: black">
        </div>
        <!--账号-->
        <!--ID-->
        <div class="form-group" style="margin-left: 0;margin-right: 0;">
            <label for="id-word" class="col-xs-2 anzhi-label text-center" style="padding-left:0;padding-right:0;background: #fff;margin-bottom: 0">
                <span class="span-1 col-xs-9 text-left  col-xs-offset-1" style="padding-left: 0">ID</span>
            </label>
            <input type="text" value="123456"  class="col-xs-10" id="id-word"  disabled style="color: black">
        </div>
        <!--ID-->
        <!--姓名-->
        <div class="form-group" style="margin-left: 0;margin-right: 0;">
            <label for="xingMing" class="col-xs-2 anzhi-label text-center" style="padding-left:0;padding-right:0;background: #fff;margin-bottom: 0">
                <span class="span-1 col-xs-9 text-left  col-xs-offset-1" style="padding-left: 0">姓名</span>
            </label>
            <input type="text" placeholder="叶赫那拉氏"  class="col-xs-10" id="xingMing" >
        </div>
        <!--姓名-->
        <!--输入身份证号码-->
        <div class="form-group" style="margin-left: 0;margin-right: 0;">
            <label for="shenFen" class="col-xs-2 anzhi-label text-center" style="padding-left:0;padding-right:0;background: #fff;margin-bottom: 0">
                <span class="span-1 col-xs-9 text-left  col-xs-offset-1" style="padding-left: 0">输入身份证号码</span>
            </label>
            <input type="text" placeholder="输入身份证号码"  class="col-xs-10" id="shenFen" >
        </div>
        <!--输入身份证号码-->
        <!--确认身份证号码-->
        <div class="form-group" style="margin-left: 0;margin-right: 0;">
            <label for="shenFen-queren" class="col-xs-2 anzhi-label text-center" style="padding-left:0;padding-right:0;background: #fff;margin-bottom: 0">
                <span class="span-1 col-xs-9 text-left  col-xs-offset-1" style="padding-left: 0">确认身份证号码</span>
            </label>
            <input type="text" placeholder="确认身份证号码"  class="col-xs-10" id="shenFen-queren" >
        </div>
        <!--确认身份证号码-->
        <!--绑定钱包地址-->
        <button type="submit" class="btn btn-block btn-danger" style="">绑定身份证</button>
        <!--绑定钱包地址-->
        <!--文字-->
        <div class="row">
            <p class="col-xs-12 text-center footer-p">钱包地址一经绑定后不可修改，若有问题请联系客服或咨询留言</p>
        </div>
        <!--文字-->
    </form>
    {include file="public/footers"}
<script>
    $("#gongneng").click(function () {
        $("#zhezhao").css("display","block")
    })
    $(".navH").click(function () {
        $("#zhezhao").css("display","none")
    })
  /*  $('.one').click(function () {
        alert(1);
        $('.two').click(function () {
            alert(2);
        })
    })*/
</script>