<script>
    /*正在卖出轮播*/
    /* window.onload比 $(function(){}) 加载的更晚一些，这样那些宽度的计算在Chrome中就可以准确计算了*/
    /*第一排*/
    function sell(){
        /*计算一个segment的宽度*/
        var segmentWidth = 0;
        $("#container #content li").each(function(){
            segmentWidth+= $(this).outerWidth(true);
        });
        $("#container #content li").clone().appendTo($("#container #content"));
        run(180000);
        function run(interval){
            $("#container #content").animate({"left":-segmentWidth}, interval,"linear",function(){
                $("#container #content").css("left",0);
                run(180000);
            });
        }
        $("#container").mouseenter(function(){
            $("#container #content").stop();
        }).mouseleave(function(){
            var passedCourse = -parseInt($("#container #content").css("left"));
            var time = 180000 * (1 - passedCourse/segmentWidth);
            run(time);
        });
    };sell()
    /*第一排*/
    /*第二排*/
    function sell_2(){
        /*计算一个segment的宽度*/
        var segmentWidth = 0;
        $("#container_2 #content_2 li").each(function(){
            segmentWidth+= $(this).outerWidth(true);
        });
        $("#container_2 #content_2 li").clone().appendTo($("#container_2 #content_2"));
        run(180000);
        function run(interval){
            $("#container_2 #content_2").animate({"left":-segmentWidth}, interval,"linear",function(){
                $("#container_2 #content_2").css("left",0);
                run(180000);
            });
        }
        $("#container_2").mouseenter(function(){
            $("#container_2 #content_2").stop();
        }).mouseleave(function(){
            var passedCourse = -parseInt($("#container_2 #content_2").css("left"));
            var time = 180000 * (1 - passedCourse/segmentWidth);
            run(time);
        });
    };sell_2()
    /*第二排*/
    /*正在卖出轮播*/


    // /*用户参与列表轮播*/
    // window.onload = function(){
    function uesr() {
        /*计算一个segment的宽度*/
        var segmentWidth_user = 0;
        $("#container-user #content-user li").each(function(){
            segmentWidth_user+= $(this).outerWidth(true);
        });

        $("#container-user #content-user li").clone().appendTo($("#container-user #content-user"));

        run(180000);
        function run(interval_user){
            $("#container-user #content-user").animate({"left":-segmentWidth_user}, interval_user,"linear",function(){
                $("#container-user #content-user").css("left",0);
                run(180000);
            });
        }
        $("#container-user").mouseenter(function(){
            $("#container-user #content-user").stop();
        }).mouseleave(function(){
            var passedCourse_user = -parseInt($("#container-user #content-user").css("left"));
            var time_user = 180000 * (1 - passedCourse_user/segmentWidth_user);
            run(time_user );
        });
    };  uesr();
    // /*用户参与列表轮播*/



    $("#gongneng").click(function () {
