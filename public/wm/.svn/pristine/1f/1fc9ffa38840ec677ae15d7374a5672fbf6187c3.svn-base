    width: 1000px;
    margin: 0 auto;
    border: 1px solid #000;
}
#inner6,#inner5,#inner4{
    width: 2800px;
    position: absolute;
    left: 0;
    top: 0;
    height: 150px;
}
#inner6 #menu6,#inner5 #menu5,#inner4 #menu4{
    float: left;
}
#inner6 li,#inner5 li,#inner4 li{
