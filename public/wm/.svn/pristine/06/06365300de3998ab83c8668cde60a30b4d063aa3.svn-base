{include file="public/header"}
<link rel="stylesheet" type="text/css" href="__STATIC__/index/css/gonggao.css">
<body >
<!--xs  sm md  lg   -->
<div class="contain" style="min-width: 300px; background-color: white">
    <!--头部-->
    <div class="row one" style="min-width: 280px;margin-right: 0;margin-left: 0;position: fixed;top: 0;z-index: 1;width: 100%">
        <div class="col-xs-4 text-left"  ><img src="__STATIC__/index/img/zhezhao.png" alt=""  style=" width: 50px;
    height: 45px;margin-top: 1rem" id="gongneng"></div>
        <div class="col-xs-4 text-center" ><img src="__STATIC__/index/img/logo.png" alt=""  style=" width: 50px;
    height: 45px;margin-top: 1rem"></div>
        <div class="col-xs-4 text-right" ><img src="__STATIC__/index/img/tuichu.png" alt=""  style=" width: 55px;
